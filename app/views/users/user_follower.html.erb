<p>フォロワー一覧</p>

<table>
  <% @user.followers.each do |user| %>
    <tr>
      <td> <%= user.nickname %> </td>
      <td><%= link_to '情報一覧へ', user_path(user) %></td>
    </tr>
  <% end %>
  <% @artist.user_followed_ids.each do |artist| %>
    <tr>
      <td> <%= artist.artistname %> </td>
      <td><%= link_to '情報一覧へ', artist_path(artist) %></td>
    </tr>
  <% end %>
</table>
<br>
<%= link_to '⬅︎ 前のページへ戻る', user_path(@user) %>