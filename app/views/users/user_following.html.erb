<p>フォロー一覧</p>

<table>
  <% @user.followings.each do |user| %>
    <tr>
      <td> <%= user.nickname %> </td>
      <td><%= link_to '情報一覧へ', user_path(user) %></td>
    </tr>
  <% end %>
  <% @user.artist_followings.each do |artist| %>
    <tr>
      <td> <%= artist.artistname %> </td>
      <td><%= link_to '情報一覧へ', artist_path(artist) %></td>
    </tr>
  <% end %>
</table>
<br>
<%= link_to '← 前のページへ戻る', user_path(@user) %>